{
	"name": "@moritzloewenstein/rsync",
	"type": "module",
	"version": "1.0.2",
	"description": "Rsync cli wrapper",
	"repository": {
		"type": "git",
		"url": "https://github.com/MoritzLoewenstein/node-rsync.git"
	},
	"files": [
		"dist"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": "./dist/index.js",
		"./package.json": "./package.json"
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"rsync",
		"wrapper",
		"cli",
		"command"
	],
	"author": "Mattijs Hoitink <mattijs@monkeyandmachine.com>",
	"license": "MIT",
	"readmeFilename": "README.md",
	"devDependencies": {
		"@biomejs/biome": "^2.2.6",
		"@types/node": "^22.18.11",
		"tsdown": "^0.15.7",
		"typescript": "^5.9.3",
		"vitest": "^3.2.4"
	},
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"typecheck": "tsc --noEmit",
		"biome:check": "biome check .",
		"biome:write": "biome check --write .",
		"biome:ci": "biome ci .",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"volta": {
		"node": "22.20.0"
	}
}
